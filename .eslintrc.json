{
  "extends": [
    "next",
    "next/core-web-vitals",
    "next/typescript",
    "plugin:storybook/recommended"
  ],
  "globals": {
    "React": "readonly"
  },
  "rules": {
    "react/no-unescaped-entities": "off",
    "no-unused-vars": "warn",
    "@typescript-eslint/no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }]

  }
}